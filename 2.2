// Eloquent JavaScript Exercise 2.2

// print numbers 1 through 100
// if number divisible by 3 print Fizz
// if number divisible by 5 print Buzz
// if number divisible by both print FizzBuzz

for(i = 1; i <= 100; i++) {
  var line = "";
  if(i % 3 === 0)
    line += "Fizz";
  if(i % 5 === 0)
    line += "Buzz";
  console.log(line || i);
}
